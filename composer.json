{
    "name": "omaghd/shipping-label",
    "description": "A Laravel package for generating shipping labels",
    "keywords": [
        "OmaghD",
        "laravel",
        "shipping-label"
    ],
    "homepage": "https://github.com/omaghd/shipping-label",
    "license": "MIT",
    "authors": [
        {
            "name": "OmaghD",
            "email": "contact@omaghd.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "spatie/laravel-package-tools": "^1.92",
        "tecnickcom/tcpdf": "^6.7"
    },
    "require-dev": {
        "larastan/larastan": "^2.0||^3.0",
        "laravel/pint": "^1.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0||^2.0",
        "rector/rector": "^1.0||^2.0"
    },
    "autoload": {
        "psr-4": {
            "OmaghD\\ShippingLabel\\": "src/"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse",
        "lint": "pint",
        "refactor": "rector",
        "test:lint": "pint --test",
        "test:types": "phpstan",
        "test:refactor": "rector --dry-run",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:refactor"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OmaghD\\ShippingLabel\\ShippingLabelServiceProvider"
            ],
            "aliases": {
                "ShippingLabel": "OmaghD\\ShippingLabel\\Facades\\ShippingLabel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
